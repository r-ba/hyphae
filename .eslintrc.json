{
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 11
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-undef": "off",
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none"
        }
      ]
    }
}
